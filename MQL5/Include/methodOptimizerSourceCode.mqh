"  {\n"
"   ZeroMemory(tradeReq);\n"
"   ZeroMemory(tradeRes);\n"
"   tradeReq.action = TRADE_ACTION_REMOVE;\n"
"   tradeReq.order = ticket;\n"
"   if(OrderSend(tradeReq, tradeRes))\n"
"     {\n"
"      return true;\n"
"     }\n"
"   return false;\n"
"  }\n"
