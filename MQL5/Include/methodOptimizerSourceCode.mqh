"                                {\n"
"                                 exceededMaxLots = !exceededMaxLots;\n"
"                                 if(exceededLotsOrders)\n"
"                                    exceededLotsOrders = 0;\n"
"                                }\n"
"                             }\n"
"                          }\n"
"                       }\n"
"                    }\n"
